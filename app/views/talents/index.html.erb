<p id="notice"><%= notice %></p>

<h1>Listing Talents</h1>

<table>
  <thead>
    <tr>
      <th>First</th>
      <th>Last</th>
      <th>Talent</th>
      <th>Description</th>
      <th>Done</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <nav class="navbar navbar-default">
      <div class="container-fluid">
        <div class="navbar-header">
          <a class="navbar-brand" href="#">WebSiteName</a>
        </div>
        <div>
          <ul class="nav navbar-nav">
            <li class="active"><a href="#">Home</a></li>
            <li class="active"><a href="#">About</a></li>
            <li class="active"><a href="#">Contact</a></li>
            <li class="active"><a href="#">Register</a></li>
            <li class="active"><a href="#">Login</a></li>
          </ul>
        </div>
      </div>
    </nav>
    <% @talents.each do |talent| %>
      <tr>
        <td><%= talent.First %></td>
        <td><%= talent.Last %></td>
        <td><%= talent.Talent %></td>
        <td><%= talent.Description %></td>
        <td><%= talent.done %></td>
        <td><%= link_to 'Show', talent %></td>
        <td><%= link_to 'Edit', edit_talent_path(talent) %></td>
        <td><%= link_to 'Destroy', talent, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Talent', new_talent_path %>
